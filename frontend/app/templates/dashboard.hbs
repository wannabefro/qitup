<h1>Dashboard</h1>

<div class="ui four column grid">
  <div class="column">
    {{#link-to 'index' class="ui fluid red card"}}
      <div class="image">
        <img src="http://placehold.it/350x350">
      </div>
      <div class="content">
        <p class="header">Your Sets</p>
      </div>
    {{/link-to}}
  </div>

  <div class="column">
    {{#link-to 'songs.index' class="ui fluid orange card"}}
      <div class="image">
        <img src="http://placehold.it/350x350">
      </div>
      <div class="content">
        <p class="header">Your Songs</p>
      </div>
    {{/link-to}}
  </div>

  <div class="column">
    {{#link-to 'index' class="ui fluid yellow card"}}
      <div class="image">
        <img src="http://placehold.it/350x350">
      </div>
      <div class="content">
        <p class="header">New Set</p>
      </div>
    {{/link-to}}
  </div>

  <div class="column">
    {{#link-to 'songs.new' class="ui fluid olive card"}}
      <div class="image">
        <img src="http://placehold.it/350x350">
      </div>
      <div class="content">
        <p class="header">New Song</p>
      </div>
    {{/link-to}}
  </div>
</div>
